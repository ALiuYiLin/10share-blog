{
  "name": "vite-vue-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "tsx build/server",
    "build": "npm run build:client && npm run build:entry:server && npm run build:server",
    "build:client": "vite build --ssrManifest .vite/ssr-manifest.json --outDir dist/client",
    "build:entry:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:server": "tsc --project tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "dev": "set NODE_ENV=development && tsx index.ts ",
    "serve": "set NODE_ENV=production && tsx index",
    "start": "set NODE_ENV=production && node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@rollup/browser": "^4.50.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@types/serve-static": "^1.15.8",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-dom": "^3.5.21",
    "@vue/compiler-sfc": "^3.5.21",
    "chokidar": "^4.0.3",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "memfs": "^4.39.0",
    "pkgroll": "^2.15.4",
    "rollup": "^4.50.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-vue": "^6.0.0",
    "serve-static": "^2.2.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-node": "^3.2.4"
  }
}
